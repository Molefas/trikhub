{
  "schemaVersion": 1,
  "id": "passthrough-trik",
  "name": "Passthrough Test Trik",
  "description": "A test trik for passthrough mode",
  "version": "1.0.0",
  "actions": {
    "details": {
      "description": "Get article details",
      "responseMode": "passthrough",
      "inputSchema": {
        "type": "object",
        "properties": {
          "id": { "type": "string" }
        },
        "required": ["id"]
      },
      "userContentSchema": {
        "type": "object",
        "properties": {
          "contentType": { "type": "string" },
          "content": { "type": "string" }
        },
        "required": ["contentType", "content"]
      }
    }
  },
  "capabilities": {
    "tools": [],
    "session": {
      "enabled": true,
      "maxDurationMs": 3600000,
      "maxHistoryEntries": 50
    }
  },
  "limits": {
    "maxExecutionTimeMs": 30000
  },
  "entry": {
    "module": "./graph.js",
    "export": "graph"
  }
}
